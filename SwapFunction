<html>
<head>
<meta charset="utf-8">
<title>Untitled Document</title>
</head>
<body>
	<form>
    <input type = "text" placeholder = "A" id="A"><br>
    <input type = "text" placeholder = "B" id="B"><br>
    <input type = "button" value = "Swap" onclick = "ss()"><br>
    <input type = "text" placeholder = "c" id = "c"><br>
    <input type = "text" placeholder = "d" id = "d"><br>
    </form>
    <script>
            function ss()
            {
              temp = document.getElementById("A").value;
              document.getElementById("c").value = document.getElementById("B").value;
              document.getElementById("d").value = temp;
            }
    </script>
</body>
</html>
